<Window x:Class="SortedListApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SortedListApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="148" Width="309">
    <Grid>
        <TextBox x:Name="keyTxt" HorizontalAlignment="Left" Height="19" Margin="43,37,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="109"/>
        <TextBox x:Name="valueTxt" HorizontalAlignment="Left" Height="19" Margin="43,72,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="109"/>
        <Button x:Name="addButton" Content="Add" HorizontalAlignment="Left" Height="54" Margin="176,37,0,0" VerticalAlignment="Top" Width="89" FontSize="16" FontWeight="Bold" Click="addButton_Click"/>

    </Grid>
</Window>
